{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/quran",
      "dest": "/server.js"
    },
    {
      "src": "/quran\\.html",
      "dest": "/server.js"
    },
    {
      "src": "/quran/surah/(\\d+)",
      "dest": "/server.js"
    },
    {
      "src": "/api/quran-data",
      "dest": "/server.js"
    }
  ]
}
